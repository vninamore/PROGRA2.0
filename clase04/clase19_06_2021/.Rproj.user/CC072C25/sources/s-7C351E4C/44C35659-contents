suma<- function(valor1, valor2=5) {
  resultado<- valor1+valor2
  return(resultado)
}
a<-5
suma(a)

potencia<- function(valor3, valor4=2) {
  resultado<- valor3^valor4
  return(resultado)
}
b<-8
potencia(b)


convert<- function(a,b=32) {
  resultado<- (a*1.8)+b
  return(resultado)
}
c<-5
convert(c)

